{
  "expo": {
    "name": "Baegopa",
    "slug": "baegopa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/baegopa-logo.png",
    "scheme": "baegopa",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/baegopa-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#027C49"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/baegopa-logo.png",
        "backgroundColor": "#027C49"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/baegopa-logo.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
